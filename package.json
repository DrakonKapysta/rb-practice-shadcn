{
  "name": "new-next-16-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss}\"",
    "type-check": "tsc --noEmit",
    "format": "npm run type-check && npm run lint && npm run prettier",
    "auth:generate": "npx @better-auth/cli@latest generate --output ./src/pkg/libraries/drizzle/schema/better-auth.ts --config ./src/pkg/integrations/better-auth/auth.ts",
    "auth:migrate": "npx @better-auth/cli@latest migrate --config ./src/pkg/integrations/better-auth/auth.ts",
    "auth:info": "npx @better-auth/cli@latest info --config ./src/pkg/integrations/better-auth/auth.ts",
    "auth:secret": "npx @better-auth/cli@latest secret",
    "db:generate": "drizzle-kit generate --config src/pkg/libraries/drizzle/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config src/pkg/libraries/drizzle/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --port=3001 --config src/pkg/libraries/drizzle/drizzle.config.ts",
    "db:push": "drizzle-kit push --config src/pkg/libraries/drizzle/drizzle.config.ts",
    "db:pull": "drizzle-kit pull --config src/pkg/libraries/drizzle/drizzle.config.ts",
    "db:check": "drizzle-kit check --config src/pkg/libraries/drizzle/drizzle.config.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-table": "^8.21.3",
    "babel-plugin-react-compiler": "^1.0.0",
    "better-auth": "^1.3.29",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "ky": "^1.13.0",
    "lucide-react": "^0.546.0",
    "next": "16.0.0",
    "next-intl": "^4.4.0",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "postgres": "^3.4.7",
    "qs-esm": "^7.0.2",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.65.0",
    "rtl-detect": "^1.1.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8.15.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/rtl-detect": "^1.0.3",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
